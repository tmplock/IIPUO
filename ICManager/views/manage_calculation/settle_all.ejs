	/**
	 * 죽장 계산
	 * 부본 : 합계 * 죽장률
	 * 대본 : (합계 - 부본죽장 - 알값) * 죽장률
	 */
	let CalculateSettle = (obj) => {
		// 수수료 계산 처리
		CalculateCommission(obj);

		// 죽장 계산
		let iSettleBaccarat = 0;
		let iSettleSlot = 0;
		let iSettlePBA = 0;
		let iSettlePBB = 0;

		if (obj.iClass == 4) {
			// 부본죽장 계산
			{
				iSettleBaccarat = parseFloat(obj.iBaccaratTotalVice ?? 0) + parseFloat(obj.iUnderOverTotalVice ?? 0);
				iSettleSlot = parseFloat(obj.iSlotTotalVice ?? 0);
				iSettlePBA = parseFloat(obj.iPBATotalVice ?? 0);
				iSettlePBB = parseFloat(obj.iPBBTotalVice ?? 0);
			}

			// 죽장계산
			iSettleBaccarat = (parseInt(obj.iBaccaratTotal) + parseInt(obj.iUnderOverTotal) - parseInt(obj.iCommissionBaccarat ?? 0) - parseInt(iSettleBaccarat)) * obj.fSettleBaccarat * 0.01;
			iSettleSlot = (obj.iSlotTotal - parseInt(obj.iCommissionSlot) - parseInt(iSettleSlot)) * obj.fSettleSlot * 0.01;
			iSettlePBA = (obj.iPBATotal - parseInt(iSettlePBA)) * obj.fSettlePBA * 0.01;
			iSettlePBB = (obj.iPBBTotal - parseInt(iSettlePBB)) * obj.fSettlePBB * 0.01;

			obj.iSettleBaccarat = iSettleBaccarat;
			obj.iSettleSlot = iSettleSlot;
			obj.iSettlePBA = iSettlePBA;
			obj.iSettlePBB = iSettlePBB;
			obj.iSettle = GetParseInt(iSettleBaccarat + iSettleSlot + iSettlePBA + iSettlePBB);
			console.log('죽장 정산');
			console.log(obj.strNickname);
			console.log(obj.iSettle);
		} else if (obj.iClass == 5) {
			let iBTotal = parseInt(obj.iBaccaratTotal) + parseInt(obj.iUnderOverTotal);
			let iSTotal = parseInt(obj.iSlotTotal);
			let iPBATotal = parseInt(obj.iPBATotal);
			let iPBBTotal = parseInt(obj.iPBBTotal);

			iSettleBaccarat = iBTotal * obj.fSettleBaccarat * 0.01;
			iSettleSlot = iSTotal * obj.fSettleSlot * 0.01;
			iSettlePBA = iPBATotal * obj.fSettlePBA * 0.01;
			iSettlePBB = iPBBTotal * obj.fSettlePBB * 0.01;

			// if (iBTotal > 0) {
			// 	iSettleBaccarat = iBTotal * obj.fSettleBaccarat * 0.01;
			// }
			// if (iSTotal > 0) {
			// 	iSettleSlot = iSTotal * obj.fSettleSlot * 0.01;
			// }
			// if (iPBATotal > 0) {
			// 	iSettlePBA = iPBATotal * obj.fSettlePBA * 0.01;
			// }
			// if (iPBBTotal > 0) {
			// 	iSettlePBB = iPBBTotal * obj.fSettlePBB * 0.01;
			// }

			obj.iSettleBaccarat = iSettleBaccarat;
			obj.iSettleSlot = iSettleSlot;
			obj.iSettlePBA = iSettlePBA;
			obj.iSettlePBB = iSettlePBB;
			obj.iSettle = GetParseInt(iSettleBaccarat + iSettleSlot + iSettlePBA + iSettlePBB);
		} else {
			iSettleBaccarat = (parseInt(obj.iBaccaratTotal) + parseInt(obj.iUnderOverTotal)) * obj.fSettleBaccarat * 0.01;
			iSettleSlot = parseInt(obj.iSlotTotal) * obj.fSettleSlot * 0.01;
			iSettlePBA = parseInt(obj.iPBATotal) * obj.fSettlePBA * 0.01;
			iSettlePBB = parseInt(obj.iPBBTotal) * obj.fSettlePBB * 0.01;

			obj.iSettleBaccarat = iSettleBaccarat;
			obj.iSettleSlot = iSettleSlot;
			obj.iSettlePBA = iSettlePBA;
			obj.iSettlePBB = iSettlePBB;
			obj.iSettle = GetParseInt(iSettleBaccarat + iSettleSlot + iSettlePBA + iSettlePBB);
		}
		console.log(obj);
	}

	/**
	 * 수수료 계산
	 * 윈로스 * 0.1(수수료)
	 * 마이너스는 윈로스, 수수료 0로 설정(미지급)
	 */
	let CalculateCommission = (obj) => {
		obj.iCommissionBaccarat = 0;
		obj.iCommissionSlot = 0;

		if (obj.iClass == 4) // 대본
		{
			let winloseB = (parseInt(obj.iBaccaratWinLose) + parseInt(obj.iUnderOverWinLose));
			if (winloseB > 0) {
				obj.iCommissionBaccarat = winloseB * 0.1;
			} else {
				obj.iBaccaratWinLose = 0;
				obj.iUnderOverWinLose = 0;
			}
		}

		let winloseS = parseInt(obj.iSlotWinLose ?? 0);
		// 슬롯 죽장이 없을 경우 알값은 지분자들이 계산
		let fSettleSlot = parseInt(obj.fSettleSlot ?? 0);
		if (winloseS > 0 && fSettleSlot != 0) {
			obj.iCommissionSlot = winloseS * 0.1;
		} else if (winloseS < 0) {
			obj.iSlotWinLose = 0;
		}
		console.log(obj);
	}



