			//RequestBettingRecord_RollingMinus(agent.iClass, agent.strGroupID, agent.iClass, agent.strID, agent.fBaccaratR, agent.fUnderOverR, agent.fSlotR);
		}
		else {
			$("#table_record_list").hide();
			$("#button_toggle_record").text(strOpen);
		}
