<div class="content" style="position:relative; padding:0 10px 0 10px;">
	<div class="write">
		<p
				style="background-repeat:no-repeat; background-position:left center; font-size:16px; color:#454545; padding-left:22px;">
			<%=__('Partner')%>
			<%=__('Information')%>
		</p>

		<table>
			<caption>
				<%=__('BoardWrite')%>
				<%=__('Register')%>
			</caption>
			<colgroup>
				<col style="width:15%;">
				<col style="width:35%;">
				<col style="width:15%;">
				<col style="width:35%;">
			</colgroup>
			<tr>
				<th scope="row">
					<%=__('Parent')%>
					<%=__('Partner')%>
				</th>
				<td colspan="3">
					<select style="vertical-align:middle;width:20%;" id="parentenable_list"
							onchange="OnChangeParent();">
						<option value=""><%=parent.strNickname%></option>
					</select>
					<input type="checkbox" id="checkbox_auto_register" value="" onclick="OnClickAutoRegister();"> 자동생성 만들기
					<input type="number" style="width:50px; height: 25px; display: none;" required="yes" id="auto_register_number" message="" placeholder="" />
					<a href="#" class="btn_red" id="check_auto_register" style="display: none;" onclick="RequestConfirmAutoRegisterValue();"><%=__('Duplication')%><%=__('OK')%></a>
					<a href="#" id="auto_register_msg" style="display: none;"><font style="font-size: 15px; color: red; font-weight: bold;">* 입력 숫자만큼 자동으로 등록됩니다.(예시: 닉네임1, 닉네임2)</font></a>
					<input type="hidden" id="strParentGroupID" value="<%=parent.strGroupID%>" />
					<input type="hidden" id="iParentClass" value="<%=parent.iClass%>" />
					<input type="hidden" id="iParentID" value="<%=parent.id%>" />
				</td>
			</tr>
			<tr>
				<th scope="row" style="background-color:lightsteelblue">
					<%=__('Nickname')%>
				</th>
				<td style="background-color:lightsteelblue">
					<input type="text" style="width:50%;" required="yes" id="strNickname" message="" placeholder="한글,숫자만 가능">&nbsp;<a
							href="#" class="btn_red" id="check_nickname">
						<%=__('Duplication')%>
						<%=__('OK')%>
					</a>
				</td>
				<th scope="row">
					<%=__('BankName')%>
				</th>
				<td>
					<input type="text" style="width:50%;" id="strBankName" required="no" message="">
				</td>
			</tr>
			<tr>
				<th scope="row" style="background-color:lightsteelblue">
					<%=__('ID')%>
				</th>
				<td style="background-color:lightsteelblue">
					<input type="text" style="width:50%;" required="yes" id="strID" message=""  placeholder="영문,숫자만 가능"/>&nbsp;<a href="#"
																																  class="btn_red" id="check_id">
						<%=__('Duplication')%>
						<%=__('OK')%>
					</a>
					<div id="check_id"></div>
				</td>
				<th scope="row">
					<%=__('BankAccount')%>
				</th>
				<td>
					<input type="text" style="width:50%;" id="strAccountNumber" required="no" message="">
				</td>
			</tr>
			<tr>
				<th scope="row">
					<%=__('Password')%>
				</th>
				<td>
					<input type="text" maxlength="16" id="strPassword" required="yes" message=""
						   style="width:50%; ime-mode:disabled"> <span
							style="display:inline-block; padding-top:4px;"></span>
				</td>
				<th scope="row">
					<%=__('BankAccountHolderName')%>
				</th>
				<td>
					<input type="text" style="width:50%;" id="strAccountOwner" required="no" message="">
				</td>
			</tr>

			<tr>
				<th scope="row">
					<%=__('PasswordConfirm')%>
				</th>
				<td>
					<input type="text" maxlength="16" id="strPasswordConfirm" required="yes" message=""
						   style="width:50%; ime-mode:disabled">
				</td>

				<th scope="row">
					<%=__('Contact')%>
				</th>
				<td colspan="3">
					<input type="text" style="width:50%;" id="strMobileNo" name="cell" required="no" message="">
				</td>
			</tr>

			<tr>
			</tr>

		</table>

		<% if ( parent.iAgentClass !=9 && parent.iAgentClass !=0 && parent.iAgentClass !=1 ) { %>
		<p style="background-repeat:no-repeat; background-position:left center; font-size:16px; color:#454545; padding-left:22px;">
			<%=__('Rolling')%>
			<%=__('Setting')%>
		</p>
		<table>
			<colgroup>
				<col width="15%" />
				<col width="35%" />
				<col width="15%" />
				<col width="35%" />
			</colgroup>

			<tr>
				<th scope="row">
					<%=__('Baccarat')%>
				</th>
				<td>
					<input type="text" style="width:40%;" required="no" message="" id="fRollingBaccarat"> %
				</td>

				<th scope="row">
					<%=__('UnderOver')%>
				</th>
				<td>
					<input type="text" style="width:40%;" required="no" message="" id="fRollingUnderOver" > %
				</td>

			</tr>

			<tr>
				<th scope="row">
					<%=__('Slot')%>
				</th>
				<td>
					<input type="text" style="width:40%;" required="no" message="" id="fRollingSlot" > %
				</td>

				<th scope="row" style="background-color:beige"></th>
				<td style="background-color:beige">
				</td>
			</tr>

		</table>
		<%}%>

		<% if (parent.iAgentClass == 8 || parent.iAgentClass == 7) { %>
		<div style="display: none">
			<table>
				<colgroup>
					<col width="15%" />
					<col width="35%" />
					<col width="15%" />
					<col width="35%" />
				</colgroup>
				<p
						style="background-repeat:no-repeat; background-position:left center; font-size:16px; color:#454545; padding-left:22px;">
					입출금 설정</p>

				<tr>


					<th scope="row">
						<%=__('UsingInput')%>
					</th>
					<td>
						<input type="checkbox" style="width:30px;height:30px;" name="using_input"
							   id="using_input" required="no" message="" checked> <strong>
							<%=__('DescUsingInput')%>
						</strong>
					</td>
					<th scope="row">
						<%=__('UsingOutput')%>
					</th>
					<td>
						<input type="checkbox" style="width:30px;height:30px;" name="using_output"
							   id="using_output" required="no" message="" checked> <strong>
							<%=__('DescUsingOutput')%>
						</strong>
					</td>

				</tr>

			</table>
		</div>
		<table>
			<colgroup>
				<col style="width:15%">
				<col style="width:35%">
				<col style="width:15%">
				<col style="width:35%">
			</colgroup>
			<p style="background-repeat:no-repeat; background-position:left center; font-size:20px; font-weight: bold; color:#454545; padding-left:22px;">PC방 이용 설정</p>
			<tr>
				<th scope="row">PC방 이용 설정</th>
				<td colspan="3">
					<input type="checkbox" style="width:30px;height:30px;color: red" name="using_input" id="using_pc" required="no" message=""> <strong>체크시 이용자 사이트 입출금 기능을 사용할 수 없습니다</strong>
				</td>
			</tr>
		</table>
		<% if ( parseInt(parent.iRootClass) <= 3 ) {%>
		<div style="">
		<% } else { %>
		<div style="display: none">
		<% } %>
			<table>
				<colgroup>
					<col style="width:15%">
					<col style="width:35%">
					<col style="width:15%">
					<col style="width:35%">
				</colgroup>
				<p style="background-repeat:no-repeat; background-position:left center; font-size:20px; font-weight: bold; color:#454545; padding-left:22px;">기타 옵션</p>
				<tr>
					<th scope="row">신규자 통장 체크 안함</th>
					<td colspan="3">
						<input type="checkbox" style="width:30px;height:30px;color: red" name="using_input" id="using_pass_new_user" required="no" message=""> <strong>체크시 입금 신청시 일반 계좌가 표시됩니다</strong>
					</td>
				</tr>
			</table>
		</div>
		<% } else { %>
		<table>
			<colgroup>
				<col width="15%" />
				<col width="35%" />
				<col width="15%" />
				<col width="35%" />
			</colgroup>
			<p style="background-repeat:no-repeat; background-position:left center; font-size:16px; color:#454545; padding-left:22px;">입출금 설정</p>
			<tr>
				<th scope="row">
					<%=__('UsingInput')%>
				</th>
				<td>
					<input type="checkbox" style="width:30px;height:30px;" name="using_input"
						   id="using_input" required="no" message="" checked> <strong>
						<%=__('DescUsingInput')%>
					</strong>
				</td>
				<th scope="row">
					<%=__('UsingOutput')%>
				</th>
				<td>
					<input type="checkbox" style="width:30px;height:30px;" name="using_output"
						   id="using_output" required="no" message="" checked> <strong>
						<%=__('DescUsingOutput')%>
					</strong>
				</td>
			</tr>
		</table>
		<div style="display: none">
			<table>
				<colgroup>
					<col style="width:15%">
					<col style="width:35%">
					<col style="width:15%">
					<col style="width:35%">
				</colgroup>
				<p style="background-repeat:no-repeat; background-position:left center; font-size:20px; font-weight: bold; color:#454545; padding-left:22px;">PC방 이용 설정</p>
				<tr>
					<th scope="row">PC방 이용 설정</th>
					<td colspan="3">
						<input type="checkbox" style="width:30px;height:30px;color: red" name="using_input" id="using_pc" required="no" message=""> <strong>체크시 이용자 사이트 입출금 기능을 사용할 수 없습니다</strong>
					</td>
				</tr>
			</table>
		</div>
		<div style="display: none">
			<table>
				<colgroup>
					<col style="width:15%">
					<col style="width:35%">
					<col style="width:15%">
					<col style="width:35%">
				</colgroup>
				<p style="background-repeat:no-repeat; background-position:left center; font-size:20px; font-weight: bold; color:#454545; padding-left:22px;">기타 옵션</p>
				<tr>
					<th scope="row">신규자 통장 체크 안함</th>
					<td colspan="3">
						<input type="checkbox" style="width:30px;height:30px;color: red" name="using_input" id="using_pass_new_user" required="no" message=""> <strong>체크시 입금 신청시 일반 계좌가 표시됩니다</strong>
					</td>
				</tr>
			</table>
		</div>
		<% } %>
		<!-- </form> -->

		<div class="write_btn align_r mt35">
			<button class="btn_modify" onclick="Submit();">
				<%=__('Save')%>
			</button>
			<a href="#" onclick="OnClickRemoveInput();" class="btn_list">
				내용 지우기
			</a>
		</div>
	</div>
</div>

<script type="text/javascript" src="js/enum.js"></script>
<script type="text/javascript" src="js/manage_partner_ref.js"></script>
<script type="text/javascript" src="js/manage_partner_popup_ref.js"></script>
<script type="text/javascript" src="js/manage_partner_popup_menu.js"></script>
<script type="text/javascript" src="js/manage_partner_popup_register.js"></script>
<script type="text/javascript">
	let agent = JSON.parse('<%-JSON.stringify(agent)%>');
	const parent = JSON.parse('<%-JSON.stringify(parent)%>');
	console.log(parent);

	const strAlertErrorRollingValue = '<%=__("AlertErrorRollingValue")%>';
	const strAlertErrorSettleValue = '<%=__("AlertErrorSettleValue")%>';
	const caAgentType = ['', '<%=__("HQ")%>', '<%=__("ViceHQ")%>', '<%=__("Admin")%>', '<%=__("ProAdmin")%>', '<%=__("ViceAdmin")%>', '<%=__("Agent")%>', '<%=__("Shop")%>', '<%=__("User")%>'];

	parentenablelist = [parent];
	$('#parentenable_list').empty();
	let tag = `<option value="${parent.strNickname}">${parent.strNickname}</option>`;
	$('#parentenable_list').append(tag);
	SetParentValues(parent.strNickname);

	$('#using_input').attr('checked', true);
	$('#using_output').attr('checked', true);
	$('#using_input_permission').attr('checked', false);

	checkBlockCharNickname(`#strNickname`);
	checkBlockCharNicknameOnlyEng(`#strID`);
	checkBlockCharNickname(`#strPassword`);
	checkBlockCharNickname(`#strPasswordConfirm`);

	checkBlockCharSpecial(`#strBankName`);
	checkBlockNum(`#strAccountNumber`);
	checkBlockCharSpecial(`#strAccountOwner`);
	checkBlockCharNickname(`#strMobileNo`);

	checkBlockNum(`#fRollingBaccarat`);
	checkBlockNum(`#fRollingUnderOver`);
	checkBlockNum(`#fRollingSlot`);

	$(document).ready(() => {
		SetUsingPC(parent.iAgentClass);
		SetAutoRegister(false);
		SetUsingPassNewUser(parent.iAgentClass);
	});

	let OnClickRemoveInput = () => {
		var result = confirm('내용을 지우시겠습니까?');
		if (result == null || result == undefined) {
			return;
		}
		bCheckID = false;
		$('#strNickname').val('');
		bCheckNickname = false;
		$('#strID').val('');
		$('#strPassword').val('');
		$('#strPasswordConfirm').val('');
		$('#strBankName').val('');
		$('#strAccountNumber').val('');
		$('#strAccountOwner').val('');
		$('#strMobileNo').val('');
		$('#fRollingBaccarat').val('');
		$('#fRollingUnderOver').val('');
		$('#fRollingSlot').val('');
		bCheckAutoRegister = false;
		bCheckIDOrNicknameAutoRegister = false;
		SetAutoRegister(false);
	}
</script>