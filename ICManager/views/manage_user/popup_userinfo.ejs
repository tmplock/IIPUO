		const fBaccaratR = $('#fBaccaratR').val();
		const fUnderOverR = $('#fUnderOverR').val();
		const fPBR = $('#fPBR').val();
		const fPBSingleR = $('#fPBSingleR').val();
		const fPBDoubleR = $('#fPBDoubleR').val();
