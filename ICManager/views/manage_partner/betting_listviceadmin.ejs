<div class="sub_wrap">
	<div class="container clearfix">

		<ul class="list_tab" style="float:right;margin-right:10px; position:relative; z-index:10;">
			<a href="#" id="register_agent" iAgentClass="5" class="btn_gray"><%=__('ViceAdmin')%><%=__('Register')%></a>
			<a href="#" id="register_agent" iAgentClass="6" class="btn_gray"><%=__('Agent')%><%=__('Register')%></a>
			<a href="#" id="register_agent" iAgentClass="7" class="btn_gray"><%=__('Shop')%><%=__('Register')%></a>
			<a href="#" id="register_agent" iAgentClass="8" class="btn_gray"><%=__('User')%><%=__('Register')%></a>
		</ul>

		<div class="content" style="position:relative; padding:0 10px 0 10px;">
			<ul class="list_tab" style="margin-bottom:10px;width:75%;z-index:10000;" id="partner_header">
			</ul>
		</div>
		
		<div class="list">
			<div id="search_date">
			</div>
		
			<div class="list_tit">
				<h3><%=__('SubMenuViceAdminList')%>
					<% if ( user.iRootClass <= 3 && user.iPermission != 100 ) { %>
					<button class="btn_blue" style="margin-left:15%;width:200px;text-align:center;background-color: #f0d17e;color:black" onclick="javascript:DoApplyRolling(user);"><%=__('Rolling')%>/<%=__('Settle')%> <%=__('Apply')%></button>
					<%}%>
					<% if ( user.iClass > 3 ) { %>
						<div style="text-align:right;margin-top:-55px;">
							<button class="btn_blue" style="border:1px solid rgb(95, 93, 93);border-radius:3px;width:70px;height:25px;text-align:center;background-color: rgb(240, 90, 90);color:white" onclick="OnClickChangeMoney('<%=user.strNickname%>', '<%=user.strGroupID%>', '<%=user.iClass%>');"><%=__('ExchangeCash')%></button>
							<span id="top_total_point_area">
								<font style="color:#00FFFF;font-size:15px;font-weight:bold;"><%=__('Cash')%> :</font><font style="color:yellow;font-size:15px;font-weight:bold;"><%=parseInt(agentinfo.iCash).toLocaleString()%></font>
								&nbsp;&nbsp;
								<font style="color:#00FFFF;font-size:15px;font-weight:bold;"><%=__('Rolling')%> :</font><font style="color:yellow;font-size:15px;font-weight:bold;"><%=parseInt(agentinfo.iRolling).toLocaleString()%></font>
								&nbsp;&nbsp;
								<font style="color:#00FFFF;font-size:15px;font-weight:bold;"><%=__('Settle')%> :</font><font style="color:yellow;font-size:15px;font-weight:bold;">0</font>
								&nbsp;&nbsp;&nbsp;
							</span>
						</div>
					<%}%>
				</h3>
			</div>

	
			<div class="search_wrap" id="div_realtimebet">
				<table class="search_list">
					<caption><%=__('SearchResult')%></caption>
					<colgroup>
						<col style="width:2%">
						<col style="width:2%">
						<col style="width:2%">
						<col style="width:2%">
						<col style="width:3%">
						<col style="width:4%">

						<col style="width:4%; color:#ffbb00">
						<col style="width:4%">
						<col style="width:4%">
						<col style="width:4%">
						<col style="width:4%">
						
						<col style="width:7%;">
						<col style="width:7%;">
						<col style="width:7%;">
						<% if (user.iRootClass <= 3 && user.iPermission != 100) { %>
							<col style="width:7%;">
						<% } %>
						<col style="width:7%;">
						<col style="width:7%;">

						<col style="width:6%;">
						<col style="width:6%;">
						<col style="width:6%;">

						<% if ( user.iRootClass <= 3 && user.iPermission != 100 ) { %>
							<col style="width:3%;">
						<% } %>
						<col style="width:6%;">

						<col style="width:3%;">
						<% if ( user.iRootClass <= 3 && user.iPermission != 100 ) { %>
							<col style="width:3%;">
							<col style="width:3%;">
						<% } %>
					</colgroup>
					<thead>
						<tr>
							<th scope="col"><%=__('ViceAdmin')%></th>
							<th scope="col"><%=__('Agent')%></th>
							<th scope="col"><%=__('Shop')%></th>
							<th scope="col"><%=__('User')%></th>
							<th scope="col"><%=__('ID')%></th>
							<th scope="col"><%=__('Nickname')%></th>

							<th scope="col" style="background-color:#f0d17e;color:black;"><%=__('Slot')%></th>
							<th scope="col" style="background-color:#f0d17e;color:black;"><%=__('Baccarat')%></th>
							<th scope="col" style="background-color:#f0d17e;color:black;"><%=__('UnderOver')%></th>
							<th scope="col" style="background-color:#f0d17e;color:black;">바카라<br>죽장</th>
							<th scope="col" style="background-color:#f0d17e;color:black;">슬롯<br>죽장</th>

							<th scope="col"><%=__('Input')%></th>
							<th scope="col"><%=__('Output')%></th>
							<th scope="col"><%=__('MyCoin')%></th>
							<% if (user.iRootClass <= 3) { %>
								<th scope="col"><%=__('WinLose')%></th>
							<% } %>
							<th scope="col"><%=__('Rolling')%></th>
							<th scope="col"><%=__('Total')%></th>

							<th scope="col">롤링</th>
							<th scope="col">미전환롤링</th>
