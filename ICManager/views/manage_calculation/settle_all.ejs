			iSettle: 0,
			iSettleVice: 0,
			iSettleTotal: 0, // 죽장 총합(대본 죽장 + 부본 죽장 + 죽장 수금)
			iSettleBeforeAcc: 0, // 전월죽장 이월(죽장 후에는 저장된 값으로 그전에는 이용자가 가지고 있는 이월 죽장값으로 표시)
			iSettleAccTotal: 0, // 총이월(해당분기 보유 이월 금액-수시로 변경됨)
			iInputQuaterSettle: 0, // 적립 죽장비
			inputSettleOption: '',
			iSettleAfter:0,
			iCommissionBaccarat: 0,
			iCommissionSlot: 0,
			iSettleQuater: 0, // 대본은 iSettle, 부본은 iSettleVice
			iSettleGive: 0,
			iSettleAcc: 0,
			iPayback: 0, // 수금액
		};
		if (bEnableSettle) {
			data.iTotal = parseInt(aObject.iBaccaratTotal) + parseInt(aObject.iUnderOverTotal) + parseInt(aObject.iSlotTotal) + parseInt(aObject.iPBATotal) + parseInt(aObject.iPBBTotal);
			data.iRolling = parseInt(aObject.iRollingMoney);
			data.iBaccaratWinLose = parseInt(aObject.iBaccaratWinLose);
			data.iUnderOverWinLose = parseInt(aObject.iUnderOverWinLose);
			data.iSlotWinLose = parseInt(aObject.iSlotWinLose);
			data.iPBWinLose = parseInt(aObject.iPBWinLose);
			data.iResult = data.iTotal - parseInt(aObject.iSettleVice ?? 0) - parseInt(aObject.iCommissionBaccarat) - parseInt(aObject.iCommissionSlot) - parseInt(aObject.iSettle);
			data.iSettleBeforeAcc = parseInt(aObject.iSettleAfter2 ?? 0);
			data.iSettleAccTotal = 0;
			data.iSettleVice = parseInt(aObject.iSettleVice ?? 0);
			data.iSettleQuater = parseInt(aObject.iSettle ?? 0);
		} else {  // 죽장 완료
			data.iTotal = parseInt(aObject.iBaccaratTotal) + parseInt(aObject.iUnderOverTotal) + parseInt(aObject.iSlotTotal) + parseInt(aObject.iPBATotal) + parseInt(aObject.iPBBTotal);
			data.iRolling = parseInt(aObject.iRollingMoney);
			data.iBaccaratWinLose = parseInt(aObject.iBaccaratWinLose);
			data.iUnderOverWinLose = parseInt(aObject.iUnderOverWinLose);
			data.iSlotWinLose = parseInt(aObject.iSlotWinLose);
			data.iPBWinLose = parseInt(aObject.iPBWinLose);
			data.iResult = data.iTotal - parseInt(aObject.iSettleVice ?? 0) - parseInt(aObject.iCommissionBaccarat) - parseInt(aObject.iCommissionSlot) - parseInt(aObject.iSettle);
			data.iSettleBeforeAcc = parseInt(aObject.iSettleBeforeAcc ?? 0);
			data.iSettleAfter = parseInt(aObject.iSettleAfter ?? 0);
			data.iSettleAccTotal = parseInt(aObject.iSettleAfter ?? 0);
			data.iSettle = parseInt(aObject.iSettle);
			data.iSettleVice = parseInt(aObject.iSettleVice ?? 0);
			data.iSettleQuater = parseInt(aObject.iSettle ?? 0);
			data.iPayback = parseInt(aObject.iPayback ?? 0);
		}

		data.iSettle = parseInt(aObject.iSettle);
		if (aObject.iClass == 5) {
			data.iSettleVice = parseInt(aObject.iSettle ?? 0);
		}

		data.iCommissionBaccarat = parseInt(aObject.iCommissionBaccarat ?? 0);
		data.iCommissionSlot = parseInt(aObject.iCommissionSlot ?? 0);
		data.iSettleGive = parseInt(aObject.iSettleGive ?? 0);
		data.iSettleAcc = parseInt(aObject.iSettleAcc ?? 0);
		// 죽장 총합
		data.iSettleTotal = (bEnableSettle == true) ? 0 : parseInt(aObject.iSettleGive ?? 0) + parseInt(aObject.iSettleAcc ?? 0) + parseInt(aObject.iPayback ?? 0);

		// 적립 죽장비
		if (bEnableSettle == true) {
			// 적립가능 금액 = 해당월 죽장 - 전월 죽장 이월
			data.iInputQuaterSettle = parseInt(aObject.iSettle ?? 0);
			if (data.iSettleBeforeAcc < 0) {
				data.iInputQuaterSettle += data.iSettleBeforeAcc;
			}

			// 마이너스는 수정 불가
			if (data.iInputQuaterSettle < 0) {
				data.inputSettleOption = 'disabled="disabled"';
			}

			// if (parseInt(aObject.iClass) == 4) {
			// 	// 마이너스는 수정 불가
			// 	if (data.iInputQuaterSettle < 0) {
			// 		data.inputSettleOption = 'disabled="disabled"';
			// 	}
			// } else if (parseInt(aObject.iClass) == 5) {
			// 	// 마이너스는 수정 불가
			// 	if (data.iInputQuaterSettle < 0) {
			// 		data.inputSettleOption = 'disabled="disabled"';
			// 	}
			// }
		} else {
			data.inputSettleOption = 'disabled="disabled"';
			data.iInputQuaterSettle = 0;
		}
		return data;
	}

